{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/application-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level}] {SourceContext} {Message} {Properties}{NewLine}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/errors-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=YourServer;Database=YourDatabaseName;User Id=YourUsername;Password=YourPassword;TrustServerCertificate=True;"
  },
  "JWTSettings": {
    "SignInKey": "YourSignInKey",
    "Audience": "YourAudiance",
    "Issuer": "YourIssuer",
    "AccessTokenExpiryInMinutes": "YourExpiryTime"
  },
  "AllowedHosts": "*"
}
